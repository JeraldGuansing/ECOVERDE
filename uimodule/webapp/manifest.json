{
  "_version": "1.12.0",
  "sap.app": {
    "id": "com.ecoverde.ECOVERDE",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "com.ecoverde.ECOVERDE.view.login",
      "type": "XML",
      "async": true,
      "id": "idAppControl"
    },
    "dependencies": {
      "minUI5Version": "1.60.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.ecoverde.ECOVERDE.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "com.ecoverde.ECOVERDE.view",
        "controlId": "idAppControl",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "name": "login",
          "pattern": "login",
          "target": [
            "login"
          ]
        },
        {
          "name": "settings",
          "pattern": "settings",
          "target": [
            "settings"
          ]
        },
        {
          "name": "main",
          "pattern": "main",
          "target": [
            "main"
          ]
        },
        {
          "name": "warehouseReport",
          "pattern": "warehouseReport",
          "target": [
            "warehouseReport"
          ]
        },
        {
          "name": "goodsReceipt",
          "pattern": "goodsReceipt",
          "target": [
            "goodsReceipt"
          ]
        }
      ],

      
      "targets": {
        "login": {
          "viewType": "XML",
          "viewLevel": 1,
          "viewName": "login"
        },
        "settings": {
          "viewType": "XML",
          "viewLevel": 1,
          "viewName": "settings"
        },
        "main": {
          "viewType": "XML",
          "viewLevel": 1,
          "viewName": "main"
        },
        "warehouseReport": {
                    "viewPath": "com.ecoverde.ECOVERDE.view",
                    "viewType": "XML",
                    "viewName": "warehouseReport",
                    "parent": "main",
                    "controlId": "pageContainer",
                    "controlAggregation": "pages",
                    "viewLevel": 3
        },
        "goodsReceipt": {
                    "viewPath": "com.ecoverde.ECOVERDE.view",
                    "viewType": "XML",
                    "viewName": "goodsreceipt100",
                    "parent": "main",
                    "controlId": "pageContainer",
                    "controlAggregation": "pages",
                    "viewLevel": 3
        }
      }
    }
  }
}

 <core:View controllerName="com.ecoverde.ECOVERDE.controller.warehouseReport"
  displayBlock="true"
  xmlns="sap.m"
	xmlns:f="sap.ui.layout.form" 
	xmlns:core="sap.ui.core" 
	xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc">
  <App id="warehouseReport" >
     <Page title="Warehouse Report" showHeader="false">
        <content>
            <VBox>
                <FlexBox
                  height="100px"
                  alignItems="Start"
                  justifyContent="Center">
                    <Image src="./resources/img/ecoverde.png"/>
                  </FlexBox>
              </VBox>
            <List headerText="Warehouse Report">
            
            <InputListItem label="Warehouse">
                <ComboBox id="whsID" 
								  items="{/value/}"
								  change="onSelectKey">
                <core:ListItem key="{WarehouseCode}" text="{WarehouseName}"/>
                </ComboBox>
              </InputListItem>
              
              <InputListItem label="From">
                  <ComboBox id="fromID" placeholder="" required="true">
                  <core:Item key="{key}" text="{text}" />
                </ComboBox>
              </InputListItem>

              <InputListItem label="To">
                <ComboBox id="toID" placeholder="" required="true">
                  <core:Item key="{key}" text="{text}" />
                </ComboBox>
              </InputListItem>
              </List>
        </content>
        <footer>
            <Bar>
                <contentMiddle>
                    <Button text="View" press="onWhseList" icon="sap-icon://tree"/>
                </contentMiddle>
            </Bar>
        </footer>
      </Page>
  </App>
</core:View>
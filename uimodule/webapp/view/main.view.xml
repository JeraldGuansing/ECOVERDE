<core:View height="100%" 
	xmlns:core="sap.ui.core" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m" 
	xmlns:chart="sap.suite.ui.microchart"
	xmlns:f="sap.f" 
	xmlns:l="sap.ui.layout" 
	xmlns:tnt="sap.tnt"
	controllerName="com.ecoverde.ECOVERDE.controller.main">
	<tnt:ToolPage id="toolPage">
		<tnt:header>
			<f:ShellBar class="myShellBar" showMenuButton="true" homeIconPressed="onIconPress" showCopilot="false"
				menuButtonPressed="onMenuButtonPress" showProductSwitcher="false">
			</f:ShellBar>
		</tnt:header>
		<tnt:sideContent>
			<tnt:SideNavigation expanded="true" selectedKey="" itemSelect="onItemSelect">
				<tnt:NavigationList items="{path: '/navigation'}">
					<tnt:NavigationListItem text="{title}" icon="{icon}" enabled="true" expanded="false" items="{items}" key="{key}">
						<tnt:NavigationListItem text="{title}" key="{key}" enabled="true"></tnt:NavigationListItem>
					</tnt:NavigationListItem>
				</tnt:NavigationList>
		
			</tnt:SideNavigation>
		</tnt:sideContent>
		<tnt:mainContents>
			<NavContainer id="pageContainer">
				<pages>
				 
				</pages>
			</NavContainer>
		</tnt:mainContents>
	</tnt:ToolPage>
	<!-- <Page showHeader="false">
		<footer>
			<Bar>
			</Bar>
		</footer>
	</Page> -->
</core:View>
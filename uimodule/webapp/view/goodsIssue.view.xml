 <mvc:View controllerName="com.ecoverde.ECOVERDE.controller.goodsIssue"
  displayBlock="true"
  xmlns="sap.m"
	xmlns:f="sap.ui.layout.form" 
	xmlns:core="sap.ui.core" 
	xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc">
  <App id="goodsIssue" >
    <Page showHeader="false" class="sapUiContentPadding sapUiResponsivePadding--header sapUiResponsivePadding--subHeader sapUiResponsivePadding--content sapUiResponsivePadding--footer">
          <VBox>
            <FlexBox
              height="100px"
              alignItems="Start"
              justifyContent="Center">
                <Image src="./resources/img/ecoverde.png"/>
              </FlexBox>
          </VBox>
   	<f:SimpleForm
          layout="ResponsiveGridLayout"
          editable="true"
          title="Goods Issue"
          adjustLabelSpan="false"
          labelSpanXL="2"
          labelSpanL="2"
          labelSpanM="2"
          labelSpanS="5">
	  </f:SimpleForm>
      <content>
        <TileContainer height="210px" id="container">
            <StandardTile icon="sap-icon://shipping-status" title="Issuance to Project" press="onSelectParamV" />
            <StandardTile icon="sap-icon://insurance-house" title="Return to Supplier" press="onSelectVendorShow" />
		  	</TileContainer>

        <!-- <Input id="myBarcode"/>
         <Button text="Scan" press="onTest"></Button> -->
          </content>
      <footer>
        <Bar>
        </Bar>
		  </footer>  
      </Page>
  </App>
</mvc:View>